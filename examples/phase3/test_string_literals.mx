package main

import "fmt"

func main() {
	// Test basic string literals
	s1 := "Hello"
	fmt.Println("s1:", s1)

	// Test empty string
	s2 := ""
	fmt.Println("s2 (empty):", s2)

	// Test string with escapes
	s3 := "Line1\nLine2\tTabbed"
	fmt.Println("s3 (escapes):", s3)

	// Test string with quotes
	s4 := "She said \"hello\""
	fmt.Println("s4 (quotes):", s4)

	// Test raw string literal (backticks)
	s5 := `Raw
String
	With Tabs`
	fmt.Println("s5 (raw):", s5)
}
